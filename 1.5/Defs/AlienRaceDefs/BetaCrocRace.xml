<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <AlienRace.ThingDef_AlienRace ParentName="BaseBetaPawn" Name="BetaCroc">
    <defName>BetaCroc</defName>
    <label>beta-humanoid (crocodile)</label>
    <description>A "Beta", a Beta Class Humanoid Lifeform. Part human and part Crocodile or Alligator.</description>
    <statBases></statBases>
    <tools Inherit="False">
      <li>
        <label>left claw</label>
        <capacities>
          <li>Scratch</li>
        </capacities>
        <power>10</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>LeftHand</linkedBodyPartsGroup>
      </li>
      <li>
        <label>right claw</label>
        <capacities>
          <li>Scratch</li>
        </capacities>
        <power>10</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>RightHand</linkedBodyPartsGroup>
      </li>
      <li>
        <label>powerful bite</label>
        <capacities>
          <li>Bite</li>
        </capacities>
        <power>23.6</power>
        <cooldownTime>3.6</cooldownTime>
        <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
        <chanceFactor>1</chanceFactor>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>14</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>11</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>0.2</chanceFactor>
      </li>
    </tools>
    <alienRace>
      <thoughtSettings>
        <cannotReceiveThoughts>
          <!-- Aquatic and cold blooded-->
          <li>SleptInHeat</li>
          <li>SoakingWet</li>
        </cannotReceiveThoughts>
        <ateThoughtSpecific></ateThoughtSpecific>
        <replacerList></replacerList>
      </thoughtSettings>
      <generalSettings>
        <alienPartGenerator>
          <headOffset>( 0,0 )</headOffset>
          <customDrawSize>( 1, 1 )</customDrawSize>
          <customHeadDrawSize>( 1, 1 )</customHeadDrawSize>
          <customPortraitDrawSize>( 1, 1 )</customPortraitDrawSize>
          <customPortraitHeadDrawSize>( 1, 1 )</customPortraitHeadDrawSize>
          <colorChannels Inherit="False">
            <li>
              <name>skin</name>
              <first Class="ColorGenerator_Options">
                <options>
                  <!-- black green -->
                  <li>
                    <weight>80</weight>
                    <only>RGBA(36, 51, 42, 255)</only>
                  </li>
                  <!-- dark green -->
                  <li>
                    <weight>10</weight>
                    <only>RGBA(78, 93, 84, 255)</only>
                  </li>
                  <!-- dark green #638c65 -->
                  <li>
                    <weight>10</weight>
                    <only>RGBA(14, 35, 22, 255)</only>
                  </li>
                  <!-- dark green #245c26-->
                  <li>
                    <weight>1</weight>
                    <only>RGBA(0.141, 0.360, 0.149,1)</only>
                  </li>
                  <!-- dark green #468b46 -->
                  <li>
                    <weight>1</weight>
                    <only>RGBA(0.274, 0.545, 0.274,1)</only>
                  </li>
                  <!-- dark green #638c65 -->
                  <li>
                    <weight>1</weight>
                    <only>RGBA(0.388, 0.549, 0.396,1)</only>
                  </li>
                </options>
              </first>
              <second Class="ColorGenerator_Options">
                <options>
                  <li>
                    <weight>1</weight>
                    <only>RGBA(169, 174, 151, 255)</only>
                  </li>
                  <li>
                    <weight>100</weight>
                    <only>RGBA(211, 217, 191, 255)</only>
                  </li>
                  <li>
                    <weight>80</weight>
                    <only>RGBA(226, 238, 187, 255)</only>
                  </li>
                  <li>
                    <weight>60</weight>
                    <only>RGBA(171, 182, 180, 255)</only>
                  </li>
                  <!-- yellow green #bbc615 -->
                  <li>
                    <weight>1</weight>
                    <only>RGBA(0.733, 0.776, 0.082,1)</only>
                  </li>
                  <!-- yellow green #b7c62a -->
                  <li>
                    <weight>1</weight>
                    <only>RGBA(0.717, 0.776, 0.164,1)</only>
                  </li>
                  <!-- yellow green #d7daa4 -->
                  <li>
                    <weight>1</weight>
                    <only>RGBA(0.843, 0.854, 0.643,1)</only>
                  </li>
                </options>
              </second>
            </li>
          </colorChannels>
        </alienPartGenerator>
        <forcedRaceTraitEntries>
          <li>
            <!-- Croc skin is very tough -->
            <defName>Tough</defName>
            <chance>100</chance>
          </li>
        </forcedRaceTraitEntries>
      </generalSettings>
      <graphicPaths>
        <apparel>
          <individualPaths>
            <li>
              <key>Apparel_WarMask</key>
              <value>Things/Pawn/Humanlike/Apparel/WarMask/WideEyes/WarMask</value>
            </li>
            <li>
              <key>Apparel_WarVeil</key>
              <value>Things/Pawn/Humanlike/Apparel/Veil/LongSnout/Veil</value>
            </li>
            <li>
              <key>Apparel_PowerArmorHelmet</key>
              <value>Things/Pawn/Humanlike/Apparel/PowerArmorHelmet/LongSnout/PowerArmorHelmet</value>
            </li>
          </individualPaths>
        </apparel>
        <head>
          <path>Things/Pawn/Humanlike/Heads/Male/BetaCroc/Male_Average_Normal</path>
          <extendedGraphics>
            <HeadType For="Male_AverageNormal">Things/Pawn/Humanlike/Heads/Male/BetaCroc/Male_Average_Normal</HeadType>
            <HeadType For="Male_AverageWide">Things/Pawn/Humanlike/Heads/Male/BetaCroc/Male_Average_Normal</HeadType>
            <HeadType For="Male_AveragePointy">Things/Pawn/Humanlike/Heads/Male/BetaCroc/Male_Narrow_Normal</HeadType>
            <HeadType For="Male_NarrowNormal">Things/Pawn/Humanlike/Heads/Male/BetaCroc/Male_Narrow_Normal</HeadType>
            <HeadType For="Male_NarrowPointy">Things/Pawn/Humanlike/Heads/Male/BetaCroc/Male_Narrow_Wide</HeadType>
            <HeadType For="Male_NarrowWide">Things/Pawn/Humanlike/Heads/Male/BetaCroc/Male_Narrow_Wide</HeadType>
            
            <HeadType For="Female_AverageNormal">Things/Pawn/Humanlike/Heads/Female/BetaCroc/Female_Average_Normal</HeadType>
            <HeadType For="Female_AveragePointy">Things/Pawn/Humanlike/Heads/Female/BetaCroc/Female_Average_Normal</HeadType>
            <HeadType For="Female_AverageWide">Things/Pawn/Humanlike/Heads/Female/BetaCroc/Female_Narrow_Normal</HeadType>
            <HeadType For="Female_NarrowNormal">Things/Pawn/Humanlike/Heads/Female/BetaCroc/Female_Narrow_Normal</HeadType>
            <HeadType For="Female_NarrowPointy">Things/Pawn/Humanlike/Heads/Female/BetaCroc/Female_Narrow_Wide</HeadType>
            <HeadType For="Female_NarrowWide">Things/Pawn/Humanlike/Heads/Female/BetaCroc/Female_Narrow_Wide</HeadType>
          </extendedGraphics>
        </head>
        <headMasks>
          <extendedGraphics>
            <HeadType For="Male_AverageNormal">Things/Pawn/Humanlike/Heads/Masks/BetaCroc/Mask_Average_Normal</HeadType>
            <HeadType For="Male_AverageWide">Things/Pawn/Humanlike/Heads/Masks/BetaCroc/Mask_Narrow_Wide</HeadType>
            <HeadType For="Male_AveragePointy">Things/Pawn/Humanlike/Heads/Masks/BetaCroc/Mask_Average_Normal</HeadType>
            <HeadType For="Male_NarrowNormal">Things/Pawn/Humanlike/Heads/Masks/BetaCroc/Mask_Narrow_Normal</HeadType>
            <HeadType For="Male_NarrowPointy">Things/Pawn/Humanlike/Heads/Masks/BetaCroc/Mask_Narrow_Normal</HeadType>
            <HeadType For="Male_NarrowWide">Things/Pawn/Humanlike/Heads/Masks/BetaCroc/Mask_Narrow_Wide</HeadType>
            
            <HeadType For="Female_AverageNormal">Things/Pawn/Humanlike/Heads/Masks/BetaCroc/Mask_Average_Normal</HeadType>
            <HeadType For="Female_AveragePointy">Things/Pawn/Humanlike/Heads/Masks/BetaCroc/Mask_Average_Normal</HeadType>
            <HeadType For="Female_AverageWide">Things/Pawn/Humanlike/Heads/Masks/BetaCroc/Mask_Narrow_Wide</HeadType>
            <HeadType For="Female_NarrowNormal">Things/Pawn/Humanlike/Heads/Masks/BetaCroc/Mask_Narrow_Normal</HeadType>
            <HeadType For="Female_NarrowPointy">Things/Pawn/Humanlike/Heads/Masks/BetaCroc/Mask_Narrow_Normal</HeadType>
            <HeadType For="Female_NarrowWide">Things/Pawn/Humanlike/Heads/Masks/BetaCroc/Mask_Narrow_Wide</HeadType>
          </extendedGraphics>
        </headMasks>
        <body>
          <extendedGraphics>
            <BodyType For="Male">Things/Pawn/Humanlike/Bodies/BetaCroc/Naked_Male</BodyType>
            <BodyType For="Female">Things/Pawn/Humanlike/Bodies/BetaCroc/Naked_Female</BodyType>
            <BodyType For="Fat">Things/Pawn/Humanlike/Bodies/BetaCroc/Naked_Fat</BodyType>
            <BodyType For="Thin">Things/Pawn/Humanlike/Bodies/BetaCroc/Naked_Thin</BodyType>
            <BodyType For="Hulk">Things/Pawn/Humanlike/Bodies/BetaCroc/Naked_Hulk</BodyType>
          </extendedGraphics>
        </body>
      </graphicPaths>
      <styleSettings Inherit="False">
        <li>
          <key>HairDef</key>
          <value>
            <hasStyle>false</hasStyle>
          </value>
        </li>
        <li>
          <key>BeardDef</key>
          <value>
            <hasStyle>false</hasStyle>
          </value>
        </li>
        <li>
          <key>TattooDef</key>
          <value>
            <hasStyle>true</hasStyle>
          </value>
        </li>
      </styleSettings>
      <raceRestriction>
        <blackGeneList MayRequire="Ludeon.RimWord.Biotech">
          <li MayRequire="Ludeon.RimWord.Biotech">Eyes_Red</li>
          <li MayRequire="Ludeon.RimWord.Biotech">Eyes_Gray</li>
        </blackGeneList>
      </raceRestriction>
    </alienRace>
    <race>
      <body>Human</body>
      <leatherDef>Leather_Lizard</leatherDef>
      <manhunterOnDamageChance>0.80</manhunterOnDamageChance>
      <manhunterOnTameFailChance>0.80</manhunterOnTameFailChance>
      <lifeStageAges>
        <li>
          <def>BetaAdult_Croc1</def>
          <minAge>18</minAge>
        </li>
        <li>
          <def>BetaAdult_Croc2</def>
          <minAge>25</minAge>
        </li>
        <li>
          <def>BetaAdult_Croc3</def>
          <minAge>30</minAge>
        </li>
        <li>
          <def>BetaAdult_Croc4</def>
          <minAge>50</minAge>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_BigBash_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_BigBash_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_BigBash_Miss</soundMeleeMiss>
      <hediffGiverSets>
        <li>BetaHumanoids_Crocodile_TraitsGiver</li>
      </hediffGiverSets>
    </race>
  </AlienRace.ThingDef_AlienRace>
