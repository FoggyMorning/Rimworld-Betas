<?xml version='1.0' encoding='utf-8'?>
<Defs>
  <PawnKindDef Abstract="True" Name="BetaHumanoids_RWP_Bandit_Recruit">
    <race>Human</race>
    <defaultFactionType>RWP_Faction_Bandit</defaultFactionType>
    <combatPower>25</combatPower>
    <initialWillRange>1~3</initialWillRange>
    <initialResistanceRange>10~16</initialResistanceRange>
    <maxGenerationAge>35</maxGenerationAge>
    <backstoryCryptosleepCommonality>0.01</backstoryCryptosleepCommonality>
    <chemicalAddictionChance>0.05</chemicalAddictionChance>
    <invNutrition>1</invNutrition>
    <invFoodDef>Pemmican</invFoodDef>
    <itemQuality>Normal</itemQuality>
    <apparelAllowHeadgearChance>0.75</apparelAllowHeadgearChance>
    <gearHealthRange>
      <min>0.75</min>
      <max>1.1</max>
    </gearHealthRange>
    <apparelMoney>
      <min>350</min>
      <max>800</max>
    </apparelMoney>
    <weaponMoney>
      <min>505</min>
      <max>650</max>
    </weaponMoney>
    <inventoryOptions>
      <skipChance>0.6</skipChance>
      <subOptionsChooseOne>
        <li>
          <choiceChance>50</choiceChance>
          <thingDef>Silver</thingDef>
          <countRange>
            <min>5</min>
            <max>15</max>
          </countRange>
        </li>
        <li>
          <choiceChance>50</choiceChance>
          <thingDef>MedicineHerbal</thingDef>
          <countRange>
            <min>1</min>
            <max>1</max>
          </countRange>
        </li>
      </subOptionsChooseOne>
    </inventoryOptions>
    <label>recruit</label>
    <apparelRequired>
      <li>RWP_Apparel_BanditScarf</li>
    </apparelRequired>
    <weaponTags>
      <li>RWPLowCapacity</li>
    </weaponTags>
    <apparelTags>
      <li>RWPBanditWear</li>
    </apparelTags>
    <backstoryFiltersOverride Inherit="False">
      <li>
        <categories>
          <li>BetaHumanoidsBackstory</li>
          <li>BetaHumanoidsBackstoryOutlander</li>
        </categories>
      </li>
    </backstoryFiltersOverride>
    <minGenerationAge>17</minGenerationAge>
  </PawnKindDef>
