<?xml version='1.0' encoding='utf-8'?>
<Defs>
  <PawnKindDef Abstract="True" Name="BetaHumanoids_ROM_TownsfolkVillager">
    <race>Human</race>
    <defaultFactionType>ROM_Townsfolk</defaultFactionType>
    <initialWillRange>1~3</initialWillRange>
    <initialResistanceRange>15~24</initialResistanceRange>
    <chemicalAddictionChance>0.025</chemicalAddictionChance>
    <invNutrition>2.55</invNutrition>
    <itemQuality>Normal</itemQuality>
    <backstoryCryptosleepCommonality>0</backstoryCryptosleepCommonality>
    <maxGenerationAge>75</maxGenerationAge>
    <modExtensions>
      <li Class="JecsTools.PawnKindGeneExtension" MayRequire="Ludeon.RimWorld.Biotech">
        <genes>
          <li>
            <defName>ROM_InnsmouthLook_Hands</defName>
            <chance>15</chance>
          </li>
        </genes>
      </li>
    </modExtensions>
    <label>villager</label>
    <combatPower>48</combatPower>
    <isFighter>false</isFighter>
    <gearHealthRange>
      <min>0.2</min>
      <max>2.0</max>
    </gearHealthRange>
    <apparelTags>
      <li>IndustrialBasic</li>
      <li>IndustrialAdvanced</li>
      <li>CallOfCthulhu_Townsfolk</li>
    </apparelTags>
    <apparelMoney>
      <min>1000</min>
      <max>2000</max>
    </apparelMoney>
    <apparelRequired>
      <li>ROM_Apparel_CallOfCthulhuBowler</li>
    </apparelRequired>
    <apparelAllowHeadgearChance>1.0</apparelAllowHeadgearChance>
    <weaponMoney>
      <min>250</min>
      <max>650</max>
    </weaponMoney>
    <weaponTags>
      <li>CallOfCthulhu_Melee</li>
      <li>CallOfCthulhu_CoastalMelee</li>
    </weaponTags>
    <techHediffsMoney>
      <min>200</min>
      <max>300</max>
    </techHediffsMoney>
    <techHediffsTags>
      <li>Simple</li>
    </techHediffsTags>
    <techHediffsChance>0.001</techHediffsChance>
    <inventoryOptions>
      <skipChance>0.75</skipChance>
      <subOptionsChooseOne>
        <li>
          <choiceChance>90</choiceChance>
          <thingDef>Silver</thingDef>
          <countRange>
            <min>25</min>
            <max>70</max>
          </countRange>
        </li>
        <li>
          <choiceChance>10</choiceChance>
          <thingDef>Silver</thingDef>
          <countRange>
            <min>700</min>
            <max>1000</max>
          </countRange>
        </li>
      </subOptionsChooseOne>
    </inventoryOptions>
    <backstoryFiltersOverride Inherit="False">
      <li>
        <categories>
          <li>BetaHumanoidsBackstory</li>
          <li>BetaHumanoidsBackstoryOutlander</li>
          <li>BetaHumanoidsBackstoryOffworld</li>
        </categories>
      </li>
    </backstoryFiltersOverride>
    <minGenerationAge>17</minGenerationAge>
    <xenotypeSet>
      <xenotypeChances>
        <BetaHumanoids_Baseliner MayRequire="Ludeon.RimWorld.Biotech">1</BetaHumanoids_Baseliner>
      </xenotypeChances>
    </xenotypeSet>
  </PawnKindDef>
