      <li>Advanced</li>
    </techHediffsTags>
    <techHediffsChance>0.25</techHediffsChance>
    <inventoryOptions>
      <skipChance>0.5</skipChance>
      <subOptionsChooseOne>
        <li>
          <thingDef>Silver</thingDef>
          <countRange>
            <min>120</min>
            <max>480</max>
          </countRange>
        </li>
        <li>
          <thingDef>MedicineIndustrial</thingDef>
          <countRange>
            <min>1</min>
            <max>2</max>
          </countRange>
        </li>
      </subOptionsChooseOne>
