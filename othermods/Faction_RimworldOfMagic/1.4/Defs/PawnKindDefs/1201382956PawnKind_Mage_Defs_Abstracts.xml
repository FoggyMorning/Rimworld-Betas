    <initialResistanceRange>10~25</initialResistanceRange>
    <label>Magic Protector</label>
    <labelPlural>magic protectors</labelPlural>
    <isFighter>true</isFighter>
    <weaponTags>
      <li>MageAttack</li>
    </weaponTags>
